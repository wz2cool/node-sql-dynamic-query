{
  "extends": "tslint:latest",

  "rules": {
    "curly": true,
    "no-console": [false],
    "no-bitwise": false,
    "variable-name": [true, "allow-leading-underscore"],
    "trailing-comma": false,
    "arrow-parens": false,
    "interface-name": false
  },
  "jsRules": {
    "curly": true
  },
  "rulesDirectory": []
}
