{
  "name": "sql-dynamic-query",
  "version": "1.0.0",
  "description": "",
  "main": "test.js",
  "scripts": {
    "pretest": "npm run deletedist && npm run copydb && tsc -p tsconfig.test.json",
    "test": "nyc mocha dist/test --recursive",
    "posttest": "npm run removenyc && del-cli coverage",
    "prepublish": "tsc",
    "cover": "tsc -p tsconfig.test.json && istanbul cover ./node_modules/mocha/bin/_mocha dist/test/**/*Test.js --recursive",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && del-cli ./coverage",
    "copydb": "npm run copysqlitedb && npm run copynothwinddb",
    "copysqlitedb": "copyfiles -f ./test/db/sqlite.db ./dist/test/db/",
    "copynothwinddb": "copyfiles -f ./test/db/northwind.db ./dist/test/db/",
    "removenyc": "del-cli .nyc_output",
    "deletedist": "del-cli dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wz2cool/sql-dynamic-query.git"
  },
  "author": "wz2cool",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wz2cool/sql-dynamic-query/issues"
  },
  "homepage": "https://github.com/wz2cool/sql-dynamic-query#readme",
  "dependencies": {
    "lodash": "^4.17.4",
    "reflect-metadata": "^0.1.10",
    "inversify": "^4.3.0"
  },
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/lodash": "^4.14.77",
    "@types/mocha": "^2.2.43",
    "@types/mysql": "^0.0.34",
    "@types/node": "^8.0.44",
    "@types/reflect-metadata": "^0.0.5",
    "@types/sqlite3": "^3.1.1",
    "@types/typescript": "^2.0.0",
    "chai": "^4.1.2",
    "copyfiles": "^1.2.0",
    "coveralls": "^2.13.2",
    "del-cli": "^1.1.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.5.3",
    "mocha-lcov-reporter": "^1.3.0",
    "mysql": "^2.15.0",
    "nyc": "^11.2.1",
    "sqlite3": "^3.1.13",
    "tslint": "^5.7.0"
  }
}